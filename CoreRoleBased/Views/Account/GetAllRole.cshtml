@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>
@{
    ViewData["Title"] = "GetAllRole";
}

<h1>List Of All Role</h1>
<div class="container">
    <a asp-action="CreateRole" asp-controller="Account">Create</a>
    @foreach (var role in Model)
    {
        <div class="card">
            <div class="card-header">
                <h4>Id:@role.Id</h4>
            </div>
            <div class="card-body">
                <h4>Role: @role.Name</h4>
            </div>
            <div class="card-footer">
                <a asp-action="EditRole" asp-controller="Account" asp-route-id="@role.Id" class="btn btn-info">Edit</a>
                <a href="#" class="btn btn-success">Delete</a>
            </div>
        </div>
    }
</div>